}
\newcommand{\proofTreeExistsE}{%
\begin{prooftree}
    \AxiomC{$\judg{\ctx}{\exists \var.\form}$}
    \AxiomC{$\judg{\ctx, \form}{\formTwo}$}
    \AxiomC{$x \notin fv(\ctx, \formTwo)$}
    \RL{\ruleExistsE}
    \TrinaryInfC{$\judg{\ctx}{\formTwo}$}
\end{prooftree}

}

