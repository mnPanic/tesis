\newcommand{\proofTreeFalseE}{%
    \begin{prooftree}
        \AxiomC{$\judg{\ctx}{\fFalse}$}
        \RL{\ruleFalseE}
        \UnaryInfC{$\judg{\ctx}{\form}$}
    \end{prooftree}
}

